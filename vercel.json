{
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/maimaidxprober/(.*)",
      "destination": "https://www.diving-fish.com/api/maimaidxprober/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}